{
  "name": "@d2-framework/template-vue",
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "node build/dev.mjs",
    "dev:test": "npm run dev --mode build.test",
    "dev:prod": "npm run dev --mode build.prod",
    "build": "node build/build.mjs --mode build.prod",
    "build:dashboard": "node build/build.mjs --mode build.prod --projects dashboard --index",
