  },
  "dependencies": {
    "@d2-framework/assets": "workspace:^",
    "@d2-framework/utils": "workspace:^",
    "vue": "^3.3.2"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^4.2.3",
    "@vitejs/plugin-vue-jsx": "^3.0.1",
    "ejs": "^3.1.9",
    "fast-glob": "^3.2.12",
    "fs-extra": "^11.1.1",
    "lodash": "^4.17.21",
    "lodash-es": "^4.17.21",
    "minimist": "^1.2.8",
    "minimist-options": "^4.1.0",
    "npm-check-updates": "^16.10.12",
    "rollup-plugin-visualizer": "^5.9.0",
    "vite": "^4.3.7",
    "vite-plugin-virtual-html": "^1.1.8"
  }
