{
  "name": "@d2-framework/d2-utils",
  "version": "0.0.0",
  "type": "module",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "types": "./dist/index.d.ts",
  "main": "./dist/index.js",
  "module": "./dist/index.esm.js",
  "exports": {
    ".": {
      "import": "./dist/index.esm.js",
      "require": "./dist/index.js"
    }
